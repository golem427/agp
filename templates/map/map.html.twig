{% extends 'base.html.twig' %}


{% block body %}
	<!-- BREADCRUMB -->
	<nav class="bg-gray-200">
		<div class="container">
			<div class="row">
				<div
					class="col-12">

					<!-- Breadcrumb -->
					<ol class="breadcrumb breadcrumb-scroll">
						<li class="breadcrumb-item">
							<a href="{{ path('home')}}" class="text-gray-700">
								Accueil
							</a>
						</li>
						<li class="breadcrumb-item active" aria-current="page">
							<a href="{{ path('map')}}" class="text-gray-700">
								Plan</a>
						</li>
					</ol>
				</div>
			</div>
			<!-- / .row -->
		</div>
		<!-- / .container -->
	</nav>

	<!-- WELCOME -->
	<section class="py-8 py-md-11 border-bottom">
		<div class="container">
			<div class="row justify-content-center">
				<div
					class="col-12 col-md-10 col-lg-8 text-center">

					<!-- Headin -->
					<h1 class="display-2">
						Notre siège social
					</h1>
					<!-- Text -->
					<p class="lead text-muted mb-7 mb-md-9">
						Venez nous rencontrer si vous le souhaitez!<br>
						4, rue de Langeac 75015 Paris | 0622454562
					</p>

				</div>
			</div>
			<!-- / .row -->
			<div class="row">
				<div class="col-12"></div>
			</div>
			<!-- / .row -->
		</div>
		<!-- / .container -->
	</section>

	<!-- LOCATION -->
	<section class="pt-8 pt-md-0 border-bottom bg-light">
		<div class="container">
			<div class="row">
				<div
					class="col-12 col-md-6">

					<!-- Map (mobile) -->
					<div class="ratio ratio-1x1 d-md-none">
						<div>
							<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2626.169254434952!2d2.289182476431094!3d48.835910171329154!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47e6706ca27ff547%3A0x7ed1c0c4a1b08de!2s4%20Rue%20de%20Langeac%2C%2075015%20Paris!5e0!3m2!1sfr!2sfr!4v1683032901715!5m2!1sfr!2sfr" width="600" height="450" padding-left="10" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
						</div>
					</div>

					<!-- Map -->
					<div
						class="position-relative h-100 vw-50 float-end d-none d-md-block">

						<div class="h-100 w-100">
							<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2626.169254434952!2d2.289182476431094!3d48.835910171329154!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47e6706ca27ff547%3A0x7ed1c0c4a1b08de!2s4%20Rue%20de%20Langeac%2C%2075015%20Paris!5e0!3m2!1sfr!2sfr!4v1683032901715!5m2!1sfr!2sfr" width="750" height="750" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
						</div>

						<!-- Shape -->
						<div class="shape shape-end shape-fluid-y text-light">
							<svg viewbox="0 0 100 1544" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M0 386V0h100v1544H50v-386L0 386z" fill="currentColor"/></svg>
						</div>

					</div>

				</div>
				<div
					class="col-12 col-md-6 col-lg-5 offset-lg-1 py-8 py-md-11 py-lg-12">

					<!-- Heading -->
					<h2>
						Notre siège est à Paris
						<br>
						<span class="text-success">
							<span data-typed='{"strings": ["Ecoute", "Satsifaction", "Expérience", "Qualité"]}'></span>.</span>
					</h2>

					<!-- Text -->
					<p class="fs-lg text-gray-700 mb-6">
						Nous nous efforçons toujours de fixer clairement les termes de nos devis et contrats et restons en contact permanent avec vous pour vous tenir informés de l'avancement de vos travaux.
					</p>

					<!-- Stats -->
					<div class="d-flex">
						<div class="pe-5">
							<h3 class="text-success mb-0">
								<span data-countup='{"startVal": 0}' data-to="100" data-aos data-aos-id="countup:in">0</span>%
							</h3>
							<p class="text-gray-700 mb-0">
								Satisfaction
							</p>
						</div>
						<div class="border-start border-gray-300"></div>
						<div class="px-5">
							<h3 class="text-success mb-0">
								<span data-countup='{"startVal": 0}' data-to="100" data-aos data-aos-id="countup:in">0</span>%
							</h3>
							<p class="text-gray-700 mb-0">
								Expérience
							</p>
						</div>
						<div class="border-start border-gray-300"></div>
						<div class="ps-5">
							<h3 class="text-success mb-0">
								<span data-countup='{"startVal": 0}' data-to="50" data-aos data-aos-id="countup:in">0</span>/<span data-countup='{"startVal": 0}' data-to="50" data-aos data-aos-id="countup:in">0</span>
							</h3>
							<p class="text-gray-700 mb-0">
								Qualité/prix
							</p>
						</div>
					</div>

				</div>
			</div>
			<!-- / .row -->
		</div>
		<!-- / .container -->
	</section>

<section class="py-8 py-md-11">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-12 col-md-10 col-lg-7 text-center">

                <!-- Heading -->
                <h2>
                  AGUADO PEINTURE
                </h2>

                <!-- Text -->
                <p class="fs-lg text-muted mb-7 mb-md-9">
                Nous vous aiderons et vous conseillerons pour<br> préparer votre projet de son ébauche à son exécution.
                </p>
                <a href="mailto:contact@aguadopeinture.fr">
            <button type="button" class="btn btn-warning lift">M'écrire</button></a>
            </div>
           
        </div> <!-- / .row -->
    </div>
</section>

{% endblock %}
