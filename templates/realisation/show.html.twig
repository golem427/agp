{% extends 'base.html.twig' %}

{% block title %}Realisation{% endblock %}

{% block body %}
    <h1>Realisation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ realisation.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ realisation.nom }}</td>
            </tr>
            <tr>
                <th>Largeur</th>
                <td>{{ realisation.largeur }}</td>
            </tr>
            <tr>
                <th>Longueur</th>
                <td>{{ realisation.longueur }}</td>
            </tr>
            <tr>
                <th>EnVente</th>
                <td>{{ realisation.enVente ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ realisation.createdAt ? realisation.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ realisation.description }}</td>
            </tr>
            <tr>
                <th>Portfolio</th>
                <td>{{ realisation.portfolio ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ realisation.slug }}</td>
            </tr>
            <tr>
                <th>File</th>
                <td>{{ realisation.file }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ realisation.updatedAt ? realisation.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ realisation.prix }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_realisation_index') }}">back to list</a>

    <a href="{{ path('app_realisation_edit', {'id': realisation.id}) }}">edit</a>

    {{ include('realisation/_delete_form.html.twig') }}
{% endblock %}
